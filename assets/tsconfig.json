{
  "compilerOptions": {
    "outDir": "../priv/static",
    "rootDir": "src",
    "sourceRoot": "src",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2015"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "noUnusedLocals": true,
  "noUnusedParameters": true
}
